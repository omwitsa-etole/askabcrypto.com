<style>
.a{
	width: 50px;
	margin-left: 1%;
}
</style>
<table class="page_list" border="1px" style="border-collapse: collapse;">
<tr border="1px">
<th class="adm">Username</th>
<th class="nm">Email</th>
<th class="con">Date created</th>
<th>Approved</th>
<th>Action</th>
</tr>
{% for account in accounts %}
	<tr border="1px">
	<td style="width: 30%;">{{ account[0] }}</td>
	<td style="width: 30%;">{{ account[1] }}</td>
	<td style="width: 15%;">{{ account[4] }}</td>
	{% if account[3] == "0"%}
		<td >False</td>
	{% else %}
		<td>True</td>
	{% endif %}
	<td ><button name="{{ account[1] }}" class="a" onclick="message(this.name)">Message</button><button class="a" style="background: red;color: white;" name="{{ account[1] }}" onclick="del(this.name)">Delete</button></td>
	</tr>
{% endfor %}
</table>